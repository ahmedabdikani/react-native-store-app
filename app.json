{
  "expo": {
    "name": "Suristore",
    "slug": "suristore",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon5.png",
    "scheme": "https",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/splash4.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ahmedabdikani.suristore"
    },
    "android": {

      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "suristore.web.app",
              "pathPrefix": "/"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon1.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.ahmedabdikani.suristore"
    },
    "web": {
      "favicon": "./src/assets/images/favicon.png"
    }
  }
}
